<?xml version="1.0" encoding="UTF-8"?>
<deploy xmlns="http://www.apache.org/ode/schemas/dd/2007/03" xmlns:axis.fbs.tudresden.io="http://axis.fbs.tudresden.io" xmlns:sample="http://eclipse.org/bpel/sample">
  <process name="sample:Light">
    <active>true</active>
    <retired>false</retired>
    <process-events generate="all"/>
    <provide partnerLink="client">
      <service name="sample:LightService" port="LightPort"/>
    </provide>
    <invoke partnerLink="FeedbackService">
      <service name="axis.fbs.tudresden.io:FeedbackService" port="FeedbackServiceHttpSoap11Endpoint"/>
    </invoke>
  </process>
</deploy>